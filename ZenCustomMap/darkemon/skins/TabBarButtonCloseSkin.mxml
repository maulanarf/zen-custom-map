<?xml version="1.0" encoding="utf-8"?>

<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="12" minHeight="12"
			 width="12" height="12"
             alpha.disabled="0.5">
     
    <fx:Metadata>
        <![CDATA[ 
        /** 
         * @copy spark.skins.spark.ApplicationSkin#hostComponent
         */
        [HostComponent("spark.components.Button")]
        ]]>
    </fx:Metadata>
          
    <!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
      
	<s:Rect left="0" right="0" top="0" bottom="0" width="12" height="12">
		<s:fill>
			<s:SolidColor alpha="0.0"/>
		</s:fill>
	</s:Rect>
	
	<s:Graphic left="1" right="1" top="1" bottom="1" width="12" height="12">
		<s:Path data="M 0 0
				      L 3 0
				      L 6 3
					  L 9 0
				      L 12 0
				      L 12 3
				      L 9 6
				      L 12 9
				      L 12 12
				      L 9 12
				      L 6 9
				      L 3 12
				      L 0 12
				      L 0 9
				      L 3 6
				      L 0 3
				      L 0 0
				      ">
			<s:stroke>
				<s:SolidColorStroke color="0x000000" weight="1"/>
			</s:stroke>
			<s:fill>
				<s:SolidColor color="0xFFFFFF"
							  color.over="0xFB7676"
							  color.down="0xFB7676"/>
			</s:fill>
		</s:Path>
	</s:Graphic>

</s:SparkButtonSkin>
